{
  "name": "redisson",
  "version": "0.0.0",
  "description": "This library is an extension of the use of Redis.",
  "license": "MIT",
  "author": {
    "name": "Toni Stark",
    "email": "nikidysbdbdy@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Toni-Stark05/redisson.git"
  },
  "homepage": "https://github.com/Toni-Stark05/redisson#readme",
  "bugs": {
    "url": "https://github.com/Toni-Stark05/redisson/issues",
    "email": "nikidysbdbdy@gmail.com"
  },
  "main": "./dist/index.js",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "nodejs",
    "redis",
    "redLock",
    "lock",
    "queue",
    "distributed"
  ],
  "engines": {
    "node": ">=12"
  },
  "browserslist": ["node >= 12"],
  "scripts": {
    "build": "rimraf ./dist && npx tsc && tsc -p tsconfig.cjs.json",
    "start": "npm run build && node dist/main",
    "format": "prettier --write \\\"{,!(node_modules)/**/}*.{js,ts}\\"
  },
  "files": [
    "scripts/*",
    "dist/index.d.ts",
    "dist/esm/*",
    "dist/cjs/*"
  ],
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/node": "^18.11.10",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
  }
}
